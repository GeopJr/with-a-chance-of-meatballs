{
  "name": "update-cloudflare-dns",
  "version": "0.4.1",
  "scripts": {
    "build": "tsc",
    "package": "npm run build; ncc build --source-map --license licenses.txt",
    "start": "npm run build; node .build/main.js",
    "dry": "npm run build; DRY_RUN=true node .build/main.js",
    "bump": "npm run package; git add dist; npm run version",
    "test": "jest --watch",
    "test-once": "jest",
    "lint": "eslint --fix src/*",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@actions/core": "1.4.0",
    "@actions/github": "5.0.0",
    "cloudflare": "2.7.0",
    "dotenv": "10.0.0",
    "fp-ts": "2.11.1",
    "hjson": "3.2.2"
  },
  "devDependencies": {
    "@decadejs/eslint-config-base": "1.0.5",
    "@types/cloudflare": "2.7.5",
    "@types/hjson": "2.4.3",
    "@types/jest": "26.0.24",
    "@types/node": "16.4.8",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "@vercel/ncc": "0.29.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "24.4.0",
    "jest": "27.0.6",
    "jest-circus": "27.0.6",
    "jest-extended": "0.11.5",
    "patch-package": "6.4.7",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "main": ".build/main.js",
  "repository": "https://github.com/pvinis/update-cloudflare-dns.git",
  "author": "Pavlos Vinieratos <pvinis@gmail.com>",
  "license": "MIT"
}
